# Docker image references for Blue and Green services
BLUE_IMAGE=yimikaade/wonderful:devops-stage-two
GREEN_IMAGE=yimikaade/wonderful:devops-stage-two

# Release identifiers passed to app containers
RELEASE_ID_BLUE=v1.0.0-blue
RELEASE_ID_GREEN=v1.0.0-green

# Default: 3000 (matches Node.js convention)
PORT=3000

# Active pool for future templating (blue or green)
ACTIVE_POOL=blue